  \verb 10.1016/j.rser.2015.11.045
  \endverb
  \field{isbn}{1364-0321}
  \field{issn}{18790690}
  \field{title}{{Complementarity of Brazils hydro and offshore wind power}}
  \field{journaltitle}{Renewable and Sustainable Energy Reviews}
  \field{year}{2016}
\endentry

