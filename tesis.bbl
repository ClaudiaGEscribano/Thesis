  \field{pages}{D00D09}
  \field{title}{{Dimming/brightening over the Iberian Peninsula: Trends in
  sunshine duration and cloud cover and their relations with atmospheric
  circulation}}
  \verb{url}
  \verb https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2008JD011394
  \endverb
  \field{volume}{114}
  \field{journaltitle}{Journal of Geophysical Research}
  \field{year}{2009}
  \warn{\item Invalid format of field 'month'}
\endentry

