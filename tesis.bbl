  \verb 10.1016/J.APENERGY.2018.11.052
  \endverb
  \field{issn}{0306-2619}
  \field{pages}{1226\bibrangedash 1234}
  \field{title}{{Empirical estimates of the radiative impact of an unusually
  extreme dust and wildfire episode on the performance of a photovoltaic plant
  in Western Mediterranean}}
